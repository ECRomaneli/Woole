{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [
                "-tunnel-response-timeout", "10s",
                "-tunnel-reconnect-timeout", "0",
                "-http", "8080"
            ]
        },
        {
            "name": "Server w/ Key",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [
                "-priv-key", "${workspaceFolder}/keys/private_key.pem",
                "-http", "8080"
            ]
        },
        {
            "name": "Server w/ Seed",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [
                "-seed", "<any-seed>",
                "-http", "8080"
            ]
        },
        {
            "name": "2m Conn Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [
                "-seed", "<any-seed>",
                "-http", "8080",
                "-tunnel-connection-timeout", "2m",
            ]
        },
        {
            "name": "Server w/ Domain",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [
                "-pattern", "{client}.woole.local",
                "-tunnel-response-timeout", "1s",
                "-tunnel-reconnect-timeout", "0",
                "-http", "8080"
            ]
        },
        {
            "name": "Client w/ Key",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "localhost",
                "-server-key", "${workspaceFolder}/keys/public_key.pem",
            ]
        },
        {
            "name": "Standalone Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-http", "8080",
                "-proxy", "https://www.w3schools.com/"
            ]
        },
        {
            "name": "10 records Standalone Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-http", "8080",
                "-proxy", "https://www.w3schools.com/",
                "-records", "10"
            ]
        },
        {
            "name": "Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "localhost",
                "-records", "0"
            ]
        },
        {
            "name": "Client 2",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "localhost",
                "-sniffer", "8002"
            ]
        },
        {
            "name": "Domain Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "test1"
            ]
        },
        {
            "name": "Domain Client 2",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "test2",
                "-sniffer", "8002"
            ]
        },
        {
            "name": "Fast Attempt Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-client", "localhost",
                "-sniffer", "8002",
                "-reconnect-attempts", "3",
                "-reconnect-interval", "1s",
            ]
        },
        {
            "name": "woole.me Client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [
                "-proxy", "https://www.w3schools.com/",
                "-tunnel", "woole.me"
            ]
        },
        {
            "name": "Server Help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "args": [ "--help" ]
        },
        {
            "name": "Client Help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/client",
            "args": [ "--help" ]
        }
    ]
}